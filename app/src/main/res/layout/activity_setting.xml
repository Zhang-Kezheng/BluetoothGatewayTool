<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activty.SettingActivity">
    <LinearLayout android:layout_width="match_parent" android:layout_height="88dp"
                  android:orientation="horizontal" app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                  android:gravity="center_vertical"
                  android:id="@+id/linearLayout3" android:layout_marginTop="20dp">
        <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp" app:srcCompat="@mipmap/setting" android:id="@+id/setting_password"
                android:layout_marginStart="20dp"/>
        <TextView
                android:layout_width="72dp"
                android:layout_height="25dp"
                android:text="参数配置"
                android:textColor="#ff4a4a4a"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="18sp"
                android:id="@+id/textView2"/>
        <TextView
                android:text="保存"
                android:layout_width="36dp"
                android:textColor="#ff4a4a4a"
                android:background="@drawable/ripple_bg"
                android:textSize="18sp"
                android:layout_height="25dp" android:id="@+id/save"
                android:layout_marginEnd="20dp"/>
    </LinearLayout>
    <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:fillViewport="true"
            android:orientation="vertical" android:id="@+id/scrollView2"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView android:layout_width="335dp" android:layout_height="480dp"
                                               app:layout_constraintTop_toTopOf="parent"
                                               app:cardCornerRadius="15dp"
                                               app:layout_constraintEnd_toEndOf="parent"
                                               app:layout_constraintStart_toStartOf="parent" android:id="@+id/cardView">

                <LinearLayout android:layout_width="match_parent"
                              android:gravity="center_horizontal"
                              android:orientation="vertical"
                              android:layout_height="480dp">
                    <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                                  android:gravity="center_vertical"
                                  android:layout_marginTop="23dp"
                                  android:orientation="horizontal">
                        <TextView
                                android:layout_width="14dp"
                                android:layout_height="20dp"
                                android:text="ID"
                                android:layout_marginStart="20dp"
                                android:textColor="#ff4a4a4a"
                                android:textSize="14sp"/>
                        <EditText android:layout_width="0dp"
                                  android:background="@null"
                                  android:layout_weight="1"
                                  android:gravity="end|center"
                                  android:hint="设备ID号（唯一号)"
                                  android:textColorHint="#ffd8d8d8"
                                  android:textSize="14sp"
                                  android:maxLength="15"
                                  android:layout_height="wrap_content" android:inputType="number"
                                  android:id="@+id/id"/>
                        <ImageView android:layout_width="24dp" android:layout_height="wrap_content"
                                   android:layout_marginStart="14dp"
                                   android:layout_marginEnd="22dp"
                                   android:src="@mipmap/scancode" android:id="@+id/scan_code"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="295dp"
                                  android:layout_height="2dp"
                                  android:background="@drawable/main_line">
                    </LinearLayout>
                    <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                                  android:layout_marginTop="10dp"
                                  android:gravity="center_vertical"
                                  android:orientation="horizontal">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:text="SSID"
                                android:layout_marginStart="20dp"
                                android:textColor="#ff4a4a4a"
                                android:textSize="14sp"/>
                        <EditText android:layout_width="0dp"
                                  android:background="@null"
                                  android:layout_weight="1"
                                  android:gravity="end|center"
                                  android:textSize="14sp"
                                  android:hint="Wifi路由器"
                                  android:textColorHint="#ffd8d8d8"
                                  android:layout_marginEnd="20dp"
                                  android:layout_height="wrap_content" android:inputType="text" android:id="@+id/ssid"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="295dp"
                                  android:layout_height="2dp"
                                  android:background="@drawable/main_line">
                    </LinearLayout>
                    <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                                  android:layout_marginTop="10dp"
                                  android:gravity="center_vertical"
                                  android:orientation="horizontal">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:text="SSID Password"
                                android:layout_marginStart="20dp"
                                android:textColor="#ff4a4a4a"
                                android:textSize="14sp"/>
                        <EditText android:layout_width="0dp"
                                  android:background="@null"
                                  android:layout_weight="1"
                                  android:gravity="end|center"
                                  android:hint="Wifi路由器密码"
                                  android:textSize="14sp"
                                  android:textColorHint="#ffd8d8d8"
                                  android:layout_marginEnd="20dp"
                                  android:layout_height="wrap_content" android:inputType="text"
                                  android:id="@+id/ssid_passowrd"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="295dp"
                                  android:layout_height="2dp"
                                  android:background="@drawable/main_line">
                    </LinearLayout>
                    <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                                  android:layout_marginTop="10dp"
                                  android:gravity="center_vertical"
                                  android:orientation="horizontal">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:text="IP"
                                android:layout_marginStart="20dp"
                                android:textColor="#ff4a4a4a"
                                android:textSize="14sp"/>
                        <EditText android:layout_width="0dp"
                                  android:background="@null"
                                  android:layout_weight="1"
                                  android:gravity="end|center"
                                  android:textSize="14sp"
                                  android:hint="目标服务器 IP 地址"
                                  android:textColorHint="#ffd8d8d8"
                                  android:layout_marginEnd="20dp"
                                  android:layout_height="wrap_content" android:inputType="number" android:id="@+id/ip"
                                  android:digits="0123456789."/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="295dp"
                                  android:layout_height="2dp"
                                  android:background="@drawable/main_line">
                    </LinearLayout>
                    <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                                  android:layout_marginTop="10dp"
                                  android:gravity="center_vertical"
                                  android:orientation="horizontal">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:text="Port"
                                android:layout_marginStart="20dp"
                                android:textColor="#ff4a4a4a"
                                android:textSize="14sp"/>
                        <EditText android:layout_width="0dp"
                                  android:background="@null"
                                  android:layout_weight="1"
                                  android:textSize="14sp"
                                  android:gravity="end|center"
                                  android:hint="目标服务器端口"
                                  android:textColorHint="#ffd8d8d8"
                                  android:layout_marginEnd="20dp"
                                  android:layout_height="wrap_content" android:inputType="number"
                                  android:id="@+id/port"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="295dp"
                                  android:layout_height="2dp"
                                  android:background="@drawable/main_line">
                    </LinearLayout>
                    <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                                  android:layout_marginTop="10dp"
                                  android:gravity="center_vertical"
                                  android:orientation="horizontal">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:text="RSSI"
                                android:layout_marginStart="20dp"
                                android:textColor="#ff4a4a4a"
                                android:textSize="14sp"/>
                        <EditText android:layout_width="0dp"
                                  android:background="@null"
                                  android:layout_weight="1"
                                  android:gravity="end|center"
                                  android:textSize="14sp"
                                  android:hint="蓝牙终端信号强度阈值"
                                  android:textColorHint="#ffd8d8d8"
                                  android:layout_marginEnd="20dp"
                                  android:layout_height="wrap_content" android:inputType="number"
                                  android:id="@+id/rssi"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="295dp"
                                  android:layout_height="2dp"
                                  android:background="@drawable/main_line">
                    </LinearLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="335dp"
                                                                       android:layout_height="40dp"
                                                                       android:layout_marginTop="10dp"
                                                                       android:gravity="center_vertical"
                                                                       android:orientation="horizontal"
                                                                       android:id="@+id/communication_type_layout">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:text="Communication Type"
                                android:textColor="#ff4a4a4a"
                                android:textSize="14sp"
                                app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="20dp"
                                android:id="@+id/textView3"/>
                        <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@mipmap/recent_back" android:id="@+id/communication_type_spinner_image"
                                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="21dp"/>
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:text="蓝牙接收频点"
                                android:textColor="#ffd8d8d8"
                                android:textSize="14sp"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="50dp"
                                android:id="@+id/communication_type_name"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <LinearLayout android:layout_width="295dp"
                                  android:layout_height="2dp"
                                  android:background="@drawable/main_line">
                    </LinearLayout>
                    <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                                  android:layout_marginTop="10dp"
                                  android:gravity="center_vertical"
                                  android:orientation="horizontal">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:text="过滤值"
                                android:layout_marginStart="20dp"
                                android:textColor="#ff4a4a4a"
                                android:textSize="14sp"/>
                        <EditText android:layout_width="0dp"
                                  android:background="@null"
                                  android:layout_weight="1"
                                  android:gravity="end|center"
                                  android:textSize="14sp"
                                  android:hint="0～65535"
                                  android:textColorHint="#ffd8d8d8"
                                  android:layout_marginEnd="20dp"
                                  android:layout_height="wrap_content" android:inputType="number"
                                  android:id="@+id/filter_value"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="295dp"
                                  android:layout_height="2dp"
                                  android:background="@drawable/main_line">
                    </LinearLayout>
                    <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                                  android:layout_marginTop="10dp"
                                  android:gravity="center_vertical"
                                  android:orientation="horizontal">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:text="MESH ID"
                                android:layout_marginStart="20dp"
                                android:textColor="#ff4a4a4a"
                                android:textSize="14sp"/>
                        <EditText android:layout_width="0dp"
                                  android:background="@null"
                                  android:layout_weight="1"
                                  android:gravity="end|center"
                                  android:textSize="14sp"
                                  android:hint="00 00 00 00 00 00"
                                  android:textColorHint="#ffd8d8d8"
                                  android:layout_marginEnd="20dp"
                                  android:layout_height="wrap_content" android:inputType="text"
                                  android:id="@+id/mesh_id"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                          android:orientation="horizontal" app:layout_constraintEnd_toEndOf="parent"
                          app:layout_constraintStart_toStartOf="parent"
                          android:gravity="center"
                          app:layout_constraintTop_toBottomOf="@+id/cardView" android:id="@+id/linearLayout14"
                          android:layout_marginTop="10dp">
                <CheckBox
                        android:text="保留项"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp" android:id="@+id/reserved_item_check_box"
                        android:button="@drawable/check_box_button"
                        android:paddingHorizontal="16dp"
                        android:layout_marginStart="20dp"
                        tools:layout_editor_absoluteY="490dp"/>
                <EditText android:layout_width="0dp" android:layout_height="wrap_content"
                          android:hint="00 00 00 00 00 00 00"
                          android:gravity="end|center"
                          android:layout_marginEnd="20dp"
                          android:background="@null"
                          android:textColor="#ffd8d8d8"
                          android:textSize="14sp"
                          android:layout_weight="1" android:id="@+id/reserved_item_edittext"/>
            </LinearLayout>
            <LinearLayout android:layout_width="295dp"
                          android:layout_height="2dp"
                          android:orientation="horizontal"
                          android:background="@drawable/main_line"
                          android:id="@+id/linearLayout13"
                          app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                          app:layout_constraintTop_toBottomOf="@+id/linearLayout14">
            </LinearLayout>
            <CheckBox
                    android:text="iBeacon broadcast（蓝牙广播）"
                    android:layout_width="335dp"
                    android:layout_height="40dp" android:id="@+id/iBeacon"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginStart="40dp" android:button="@drawable/check_box_button"
                    android:paddingHorizontal="16dp"
                    android:layout_marginTop="10dp" app:layout_constraintTop_toBottomOf="@+id/linearLayout13"/>

            <CheckBox
                    android:text="蓝牙广播接收"
                    android:layout_width="335dp"
                    android:layout_height="40dp" app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/iBeacon" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginStart="40dp" android:layout_marginTop="10dp" android:id="@+id/bluetooth_accept"
                    android:button="@drawable/check_box_button" android:paddingHorizontal="16dp"/>

            <androidx.cardview.widget.CardView android:layout_width="335dp" android:layout_height="60dp"
                                               app:cardCornerRadius="15dp"
                                               app:layout_constraintEnd_toEndOf="parent"
                                               app:layout_constraintStart_toStartOf="parent"
                                               android:id="@+id/setting_agreement_type_card"
                                               app:layout_constraintTop_toBottomOf="@+id/bluetooth_accept"
                                               android:layout_marginTop="10dp">
                <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="335dp"
                                                                   android:layout_height="40dp"
                                                                   android:layout_marginTop="10dp"
                                                                   android:gravity="center_vertical"
                                                                   android:orientation="horizontal">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:text="协议类型"
                            android:textColor="#ff4a4a4a"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="20dp"
                            android:id="@+id/textView6"/>
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/recent_back"
                            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="21dp"
                            android:id="@+id/agreement_type_spinner_image"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:textColor="#ff3993cf"
                            android:textSize="14sp"
                            app:layout_constraintEnd_toStartOf="@+id/agreement_type_spinner_image"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="14dp"
                            android:id="@+id/agreement_type_name"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView android:layout_width="335dp" android:layout_height="60dp"
                                               app:cardCornerRadius="15dp"
                                               app:layout_constraintEnd_toEndOf="parent"
                                               app:layout_constraintStart_toStartOf="parent"
                                               android:id="@+id/setting_dhcp_ip_card"
                                               app:layout_constraintTop_toBottomOf="@+id/setting_agreement_type_card"
                                               android:layout_marginTop="10dp">
                <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="335dp"
                                                                   android:layout_height="40dp"
                                                                   android:layout_marginTop="10dp"
                                                                   android:gravity="center_vertical"
                                                                   android:orientation="horizontal">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:text="DHCP &amp; 固定IP"
                            android:textColor="#ff4a4a4a"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="20dp"
                            android:id="@+id/textView5"/>
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/recent_back" android:id="@+id/dhcp_ip_spinner_image"
                            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="21dp"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:textColor="#ff3993cf"
                            android:textSize="14sp"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/dhcp_ip_spinner_image"
                            android:layout_marginEnd="14dp"
                            android:id="@+id/dhcp_ip_name"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView android:layout_width="335dp" android:layout_height="60dp"
                                               app:cardCornerRadius="15dp"
                                               app:layout_constraintEnd_toEndOf="parent"
                                               app:layout_constraintStart_toStartOf="parent"
                                               android:id="@+id/setting_scene_card"
                                               android:layout_marginTop="10dp"
                                               app:layout_constraintTop_toBottomOf="@+id/setting_dhcp_ip_card">
                <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="335dp"
                                                                   android:layout_height="40dp"
                                                                   android:layout_marginTop="10dp"
                                                                   android:gravity="center_vertical"
                                                                   android:orientation="horizontal">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:text="设备安装位置"
                            android:textColor="#ff4a4a4a"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="20dp"
                            android:id="@+id/textView4"/>
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/recent_back"
                            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="21dp"
                            android:id="@+id/scene_spinner_image"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:textColor="#ff3993cf"
                            android:textSize="14sp"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/scene_spinner_image"
                            android:layout_marginEnd="14dp"
                            android:id="@+id/scene_name"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                          android:orientation="horizontal" app:layout_constraintTop_toBottomOf="@+id/setting_scene_card"
                          app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                          android:id="@+id/linearLayout" android:layout_marginTop="10dp">
                <CheckBox
                        android:text="第六包"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp" android:id="@+id/six_box"
                        android:layout_marginStart="20dp"
                        tools:layout_editor_absoluteY="860dp" android:button="@drawable/check_box_button"
                        android:paddingHorizontal="16dp"/>
                <EditText android:layout_width="0dp"
                          android:background="@null"
                          android:layout_weight="1"
                          android:textSize="14sp"
                          android:gravity="end|center"
                          android:hint="输入"
                          android:textColorHint="#ffd8d8d8"
                          android:layout_marginEnd="20dp"
                          android:layout_height="wrap_content" android:inputType="text" android:id="@+id/six_content"/>
            </LinearLayout>
            <LinearLayout android:layout_width="295dp"
                          android:layout_height="2dp"
                          android:orientation="horizontal"
                          android:background="@drawable/main_line"
                          app:layout_constraintEnd_toEndOf="parent"
                          app:layout_constraintStart_toStartOf="parent"
                          app:layout_constraintTop_toBottomOf="@+id/linearLayout" android:id="@+id/linearLayout2"/>
            <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                          android:orientation="horizontal"
                          app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                          app:layout_constraintTop_toBottomOf="@+id/linearLayout" android:id="@+id/linearLayout5"
                          android:layout_marginTop="10dp">
                <CheckBox
                        android:text="第七包"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginStart="20dp"
                        tools:layout_editor_absoluteY="860dp" android:id="@+id/seven_box"
                        android:button="@drawable/check_box_button" android:paddingHorizontal="16dp"/>
                <EditText android:layout_width="0dp"
                          android:background="@null"
                          android:layout_weight="1"
                          android:textSize="14sp"
                          android:gravity="end|center"
                          android:hint="输入"
                          android:textColorHint="#ffd8d8d8"
                          android:layout_marginEnd="20dp"
                          android:layout_height="wrap_content" android:inputType="text"
                          android:id="@+id/seven_content"/>
            </LinearLayout>
            <LinearLayout android:layout_width="295dp"
                          android:layout_height="2dp"
                          android:orientation="horizontal"
                          android:background="@drawable/main_line"
                          app:layout_constraintEnd_toEndOf="parent"
                          app:layout_constraintStart_toStartOf="parent"
                          app:layout_constraintTop_toBottomOf="@+id/linearLayout5" android:id="@+id/linearLayout6"/>
            <LinearLayout android:layout_width="335dp" android:layout_height="40dp"
                          android:orientation="horizontal"
                          app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                          app:layout_constraintTop_toBottomOf="@+id/linearLayout6" android:layout_marginTop="10dp"
                          android:id="@+id/linearLayout7">
                <CheckBox
                        android:text="第八包"
                        android:layout_width="wrap_content"
                        android:paddingHorizontal="16dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="20dp"
                        tools:layout_editor_absoluteY="860dp" android:id="@+id/eight_box"
                        android:button="@drawable/check_box_button"/>
                <EditText android:layout_width="0dp"
                          android:background="@null"
                          android:layout_weight="1"
